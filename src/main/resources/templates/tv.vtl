#foreach($result in $results)
<img src="https://image.tmdb.org/t/p/w500$result.getPosterPath()" alt="img" width="100px" height="100px">
<a href="/series/$result.getId()" id="id"><h1>Name: $result.getOriginalName()</h1></a>
<h2>Vote Count: $result.getVoteCount()</h2>
<h3>Overview:</h3>
<p>$result.getOverview()</p>
<a href="https://www.youtube.com/embed?listType=search&list=$result.getName()SeasonTrailer" class="btn btn-lg btn-info">Watch Preview</a>
#end
<a href="/popular"><button class="btn btn-info btn">View Latest Tv Series</button></a>
